{
  "name": "redisai-js",
  "version": "0.0.4",
  "description": "A high-performance Typescript client for RedisAI",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "tsc && nyc mocha --require ts-node/register --require source-map-support/register --recursive tests/** "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipecosta90/redisai-js.git"
  },
  "keywords": [
    "redis",
    "redisai",
    "javascript-client",
    "serving-tensors",
    "machine-learning"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": "filipecosta.90@gmail.com",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/filipecosta90/redisai-js/issues"
  },
  "homepage": "https://github.com/filipecosta90/redisai-js#readme",
  "dependencies": {
    "@types/redis": "^2.8.18",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.9.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
